<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/player%20UI/player%20UI.uss?fileID=7433441132597879392&amp;guid=a902171ed7ea5a440b2600c15b53b6c8&amp;type=3#player UI" />
    <ui:VisualElement name="screen" style="align-items: flex-end; width: 100%; height: 100%;">
        <ui:VisualElement name="stats_screen" style="width: 300px; background-color: rgb(46, 46, 46); padding-right: 10px; padding-left: 10px; max-height: none; padding-bottom: 10px; display: none; position: absolute;">
            <ui:VisualElement name="stats">
                <ui:Label text="LEVEL: 1" display-tooltip-when-elided="true" name="level" enable-rich-text="true" class="text_box" />
                <ui:Label text="STATS" display-tooltip-when-elided="true" name="stats_heading" class="text_heading" />
                <ui:Label text="STRENGTH: 10" display-tooltip-when-elided="true" name="strength_stat" enable-rich-text="true" class="text_box" />
                <ui:Label text="ENDURANCE: 10" display-tooltip-when-elided="true" name="endurance_stat" enable-rich-text="true" class="text_box" />
                <ui:Label text="DEXTERITY: 10" display-tooltip-when-elided="true" name="dexterity_stat" enable-rich-text="true" class="text_box" />
                <ui:Label text="AGILITY: 10" display-tooltip-when-elided="true" name="agility_stat" enable-rich-text="true" class="text_box" />
                <ui:Label text="MAGIC: 10" display-tooltip-when-elided="true" name="magic_stat" enable-rich-text="true" class="text_box" />
            </ui:VisualElement>
            <ui:VisualElement name="experience" style="width: auto; background-color: rgb(46, 46, 46); padding-right: 0; padding-left: 0; max-height: none; padding-bottom: 0; padding-top: 0; display: flex;">
                <ui:Label text="EXPERIENCE" display-tooltip-when-elided="true" name="experience" class="text_heading" />
                <ui:Label text="STRENGTH XP: 10" display-tooltip-when-elided="true" name="strength_experience" enable-rich-text="true" class="text_box" />
                <ui:Label text="ENDURANCE XP: 10" display-tooltip-when-elided="true" name="endurance_experience" enable-rich-text="true" class="text_box" />
                <ui:Label text="DEXTERITY XP: 10" display-tooltip-when-elided="true" name="dexterity_experience" enable-rich-text="true" class="text_box" />
                <ui:Label text="AGILITY XP: 10" display-tooltip-when-elided="true" name="agility_experience" enable-rich-text="true" class="text_box" />
                <ui:Label text="MAGIC XP: 10" display-tooltip-when-elided="true" name="magic_experience" enable-rich-text="true" class="text_box" />
            </ui:VisualElement>
            <ui:Button text="LEVEL UP" display-tooltip-when-elided="true" name="levelup" class="button" style="display: none;" />
            <ui:Button text="STATUS UPDATE" display-tooltip-when-elided="true" name="status_update" class="button" style="display: none;" />
        </ui:VisualElement>
        <ui:VisualElement name="abilities" />
        <ui:VisualElement name="footer" style="width: 100%; align-items: center; position: absolute; bottom: 0; height: 8%; background-color: rgb(77, 77, 77);">
            <ui:VisualElement name="healthbar_container" style="width: 100%; height: 8px; align-items: flex-start; opacity: 1;">
                <ui:VisualElement name="healthbar" style="width: 100%; height: 8px; background-color: rgb(29, 255, 0); opacity: 1;" />
            </ui:VisualElement>
            <ui:VisualElement name="hotbar" style="width: 100%; height: 100%; align-items: flex-start; flex-direction: row; justify-content: center;" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
